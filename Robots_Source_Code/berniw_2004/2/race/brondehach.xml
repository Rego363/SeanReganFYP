<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="" type="template" mode="mw">
  <section name="Car">
    <attnum name="initial fuel" unit="l" min="1.0" max="87.0" val="87.0"/>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="11" val="11"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="16" val="11"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.2"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.4"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.7"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.28"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="1.05"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.9"/>
      </section>
    </section>
  </section>

  	<section name="Front Right Suspension">
