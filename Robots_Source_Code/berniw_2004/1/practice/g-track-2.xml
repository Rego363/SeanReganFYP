<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="" type="template" mode="mw">
  <section name="Car">
    <attnum name="initial fuel" unit="l" min="1.0" max="87.0" val="87.0"/>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="0" max="11" val="11"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="0" max="16" val="13"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.58"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="40000" val="22000"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.2"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.4"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.65"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.23"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="1.0"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.82"/>
      </section>
    </section>
  </section>

  <section name="berniw private">
    <attnum name="caero" val="1.5"/>
    <attnum name="cfriction" val="1.2"/>
    <attnum name="steerpgain" val="0.15"/>
    <attnum name="steerpgainmax" val="0.4"/>
    <attnum name="fuelperlap" val="2.02"/>
    <attstr name="teammate" val="berniw 2004 2"/>
  </section>

</params>















